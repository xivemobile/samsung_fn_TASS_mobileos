clear
   echo ""
   echo "==========================================="
   echo "==         Fly-On Mod Main script        =="
   echo "==           Slaid480@XDA 2013           =="
   echo "==========================================="
sleep 2
   echo ""
   echo "checking if files are correctly installed,please wait..."
sleep 2
   echo ""
   echo "Checking for init.d folder..."
sleep 1
if [ -e /system/etc/init.d ]; then
   echo ""
   echo "init.d folder was found..."
sleep 1
else
   echo ""
   echo "init.d flder was not found,please try reinstalling it then re-flashing Fly-On Mod"
sleep 1
fi

   echo ""
   echo "Checking for entropy_on binary..."
sleep 1
if [ -e /system/bin/entropy_on ]; then
   echo ""
   echo "entropy_on binary is installed..."
sleep 1
else
   echo ""
   echo "entropy_on binary was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for entropy_off binary..."
sleep 1
if [ -e /system/bin/entropy_off ]; then
   echo ""
   echo "entropy_off binary is installed..."
sleep 1
else
   echo ""
   echo "entropy_off binary was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for uninstaller binary..."
sleep 1
if [ -e /system/bin/uninstaller ]; then
   echo ""
   echo "uninstaller binary is installed..."
sleep 1
else
   echo ""
   echo "uninstaller binary was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for openvpn binary..."
sleep 1
if [ -e /system/xbin/openvpn ]; then
   echo ""
   echo "openvpn binary is installed..."
sleep 1
else
   echo ""
   echo "openvpn binary was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for zipalign binary..."
sleep 1
if [ -e /system/xbin/zipalign ]; then
   echo ""
   echo "zipalign binary is installed..."
sleep 1
else
   echo ""
   echo "zipalign binary was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for entro binary..."
sleep 1
if [ -e /system/etc/rngd/entro ]; then
   echo ""
   echo "entro binary is installed..."
sleep 1
else
   echo ""
   echo "entro binary was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for rngd binary..."
sleep 1
if [ -e /system/etc/rngd/rngd ]; then
   echo ""
   echo "rngd binary is installed..."
sleep 1
else
   echo ""
   echo "rngd binary was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for entropy script..."
sleep 1
if [ -e /system/etc/rngd/entropy ]; then
   echo ""
   echo "entropy script is installed..."
sleep 1
else
   echo ""
   echo "entropy script was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for hosts script..."
sleep 1
if [ -e /system/etc/hosts ]; then
   echo ""
   echo "hosts script is installed..."
sleep 1
else
   echo ""
   echo "hosts script was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for 09sdcardspeedfix script..."
sleep 1
if [ -e /system/etc/init.d/09sdcardspeedfix ]; then
   echo ""
   echo "09sdcardspeedfix script is installed..."
sleep 1
else
   echo ""
   echo "09sdcardspeedfix script was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for 92cleaner script..."
sleep 1
if [ -e /system/etc/init.d/92cleaner ]; then
   echo ""
   echo "92cleaner script is installed..."
sleep 1
else
   echo ""
   echo "92cleaner script was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for 94Governor_tweaks script..."
sleep 1
if [ -e /system/etc/init.d/94Governor_tweaks ]; then
   echo ""
   echo "94Governor_tweaks script is installed..."
sleep 1
else
   echo ""
   echo "94Governor_tweaks script was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for 98fly_engine script..."
sleep 1
if [ -e /system/etc/init.d/98fly_engine ]; then
   echo ""
   echo "98fly_engine script is installed..."
sleep 1
else
   echo ""
   echo "98fly_engine script was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for darky_zipalign script..."
sleep 1
if [ -e /system/etc/init.d/darky_zipalign ]; then
   echo ""
   echo "darky_zipalign script is installed..."
sleep 1
else
   echo ""
   echo "darky_zipalign was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for ram_manager script..."
sleep 1
