   echo ""
   echo "Checking for sqlite_optimize script..."
sleep 1
if [ -e /system/etc/init.d/sqlite_optimize ]; then
   echo ""
   echo "sqlite_optimize script is installed..."
sleep 1
else
   echo ""
   echo "sqlite_optimize was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for zipaligndata script..."
sleep 1
if [ -e /system/etc/init.d/zipaligndata ]; then
   echo ""
   echo "zipaligndata script is installed..."
sleep 1
else
   echo ""
   echo "zipaligndata was not found,please Re-flash Fly-On Mod then try again!"
sleep 1
fi

   echo ""
   echo "Checking for GPL License..."
sleep 1
